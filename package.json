{
  "name": "react-freeform",
  "version": "0.0.1",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src --ext .jsx --ext .js",
    "clean:lib": "rimraf ./lib/*",
    "compile:lib": "babel src --out-dir lib",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build:lib": "npm run clean:lib && npm run compile:lib",
    "build": "npm run build:lib",
    "dev:examples": "webpack-dev-server --config ./webpack/examples.config.babel.js --hot",
    "test": "NODE_ENV=test nyc npm run test:raw",
    "test:raw": "NODE_ENV=test mocha --require babel-register --require test/test_helper.js --recursive test/**/*.{js,jsx}",
    "test:single": "NODE_ENV=test mocha --require babel-register --require test/test_helper.js --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SpenserJ/react-freeform.git"
  },
  "author": "Spenser Jones <hello@spenserj.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SpenserJ/react-freeform/issues"
  },
  "homepage": "https://github.com/SpenserJ/react-freeform#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "css-loader": "^0.28.5",
    "css-modules-require-hook": "^4.0.6",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "jsdom": "^11.6.2",
    "mocha": "^5.0.5",
    "node-sass": "^4.5.3",
    "nyc": "^11.6.0",
    "raw-loader": "^0.5.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "^6.0.6",
    "sinon": "^4.4.8",
    "style-loader": "^0.18.2",
    "validator": "^8.1.0",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0",
    "yargs": "^8.0.2"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "lodash.memoize": "^4.1.2",
    "object-path-immutable": "^1.0.1",
    "prop-types": "^15.5.10",
    "react-transition-group": "^1.2.0"
  },
  "nyc": {
    "require": [
      "babel-register",
      "./test/test_helper.js"
    ],
    "exclude": [
      "docs"
    ],
    "include": [
      "src/**/*.{js,jsx}"
    ],
    "extension": [
      ".jsx"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": false,
    "cache": true,
    "all": true
  }
}
